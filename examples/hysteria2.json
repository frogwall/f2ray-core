{
  "log": {
    "loglevel": "info"
  },
  "inbounds": [
    {
      "port": 1080,
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "hysteria2",
      "settings": {
        "servers": [
          {
            "address": "example.com",
            "port": 443,
            "password": "your-password"
          }
        ],
        "bandwidth": {
          "max_tx": 104857600,
          "max_rx": 104857600
        },
        "ignore_client_bandwidth": false
      },
      "streamSettings": {
        "security": "tls"
      },
      "tag": "hy2-out"
    }
  ]
}